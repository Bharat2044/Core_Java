package com.bharat01.if_statement.simple_if;

public class IfStatementEx2 {
    public static void main(String[] args) {
        int i = 10;
  
      	if (i > 15) {
      		System.out.println("I am inside the if-block");
      		System.out.println("10 is greater than 15");
      	}
      	
      	System.out.println("I am Not inside if-block");
    }
}